{
  "name": "prefect",
  "version": "0.1.0",
  "description": "The campus expert github bot",
  "scripts": {
    "start": "probot run",
    "pretty": "prettier --write '{,!(node_modules)/**/}*.js'",
    "test":
      "prettier --list-different '{,!(node_modules)/**/}*.js' && mocha 'tests/**/*.test.js'"
  },
  "author": "Elliot Blackburn <elliot.blackburn@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "mocha": "^3.5.3",
    "probot": "^0.9.1",
    "should": "^13.1.0"
  },
  "probot": {
    "plugins": ["./scripts/template-enforcer"]
  },
  "devDependencies": {
    "prettier": "^1.7.2"
  }
}
